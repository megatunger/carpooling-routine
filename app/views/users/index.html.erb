<h2>Users</h2>
<div class="table-responsive">
  <table class="table table-striped table-sm">
    <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Updated</th>
    </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.user_name %></td>
        <td><%= user.created_at %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>